@page "/Console"
<MudCard>
    <MudCardHeader>
        <MudText Typo="Typo.h5">Konsole</MudText>
    </MudCardHeader>
    <MudDivider />
    <MudCardContent Class="overflow-scroll">
        <MudText>This is the console page.</MudText>
    </MudCardContent>
    <MudDivider />
    <MudStack Row="true">
        <MudSelect @bind-Value="enumValue" Label="Select mode" HelperText="Enum" OpenIcon="@Icons.Material.Filled.LocalDrink" AdornmentColor="Color.Secondary">
            @foreach (ConsoleModes item in Enum.GetValues(typeof(ConsoleModes)))
            {
                <MudSelectItem Value="@item">@item</MudSelectItem>

            }
        </MudSelect>
        <MudTextField @bind-Value="TextValue" Label="Outlined" Variant="Variant.Filled"></MudTextField>
    </MudStack>
</MudCard>